{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputLogs": "new-only"
    },
    "migrate": {
      "cache": false
    },
    "precommit": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
